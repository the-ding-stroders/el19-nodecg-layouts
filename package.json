{
  "name": "tds-2020-layouts",
  "version": "1.0.0",
  "description": "Our NodeCG bundle for the Extra Life 2018 Streamaganza",
  "homepage": "https://dingstroders.com",
  "author": {
    "name": "David Thomas",
    "email": "dev@synth3.tk",
    "url": "https://synth3.tk"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:dashboard": "cross-env NODE_ENV=production webpack --config ./extension/webpack-dashboard.js",
    "build:graphics": "cross-env NODE_ENV=production webpack --config ./extension/webpack-graphics.js",
    "pretest": "eslint --ignore-path .gitignore .",
    "watch": "run-p watch:*",
    "watch:dashboard": "webpack -w --config ./extension/webpack-dashboard.js",
    "watch:graphics": "webpack -w --config ./extension/webpack-graphics.js"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "nodecg",
    "extralife",
    "donations",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.6.0",
    "bundleDependencies": {
      "lfg-twitchapi": "^2.0.0"
    },
    "dashboardPanels": [
      {
        "name": "music-player",
        "title": "Music Player",
        "width": 3,
        "file": "music-player.html",
        "workspace": "Overview"
      },
      {
        "name": "cta",
        "title": "Call-To-Actions",
        "width": 5,
        "file": "cta.html",
        "workspace": "Overview"
      },
      {
        "name": "logo-selector",
        "title": "Logo Selector",
        "width": 7,
        "file": "logo-selector.html",
        "workspace": "Setup"
      },
      {
        "name": "schedule",
        "title": "Schedule",
        "file": "schedule.html",
        "fullbleed": true
      }
    ],
    "graphics": [
      {
        "file": "lower_bar.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      },
      {
        "file": "layout.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      },
      {
        "file": "idle.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      }
    ],
    "assetCategories": [
      {
        "name": "tdslogos",
        "title": "TDS Logos",
        "allowedTypes": [
          "png"
        ]
      },
      {
        "name": "charitylogos",
        "title": "Charity Logos",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT",
  "engines": {
    "yarn": ">= 1.0.0"
  },
  "dependencies": {
    "@bower_components/jquery": "jquery/jquery-dist#^3.3.1",
    "@bower_components/jquery-animateNumber": "aishek/jquery-animateNumber#^0.0.14",
    "@bower_components/jquery-textfill": "jquery-textfill/jquery-textfill#^0.6.0",
    "@bower_components/jquery-ui": "components/jqueryui#^1.12.1",
    "@bower_components/letteringjs": "davatron5000/Lettering.js#^0.7.0",
    "@bower_components/moment": "moment/moment#^2.22.2",
    "@bower_components/vue": "vuejs/vue#^2.5.17",
    "@bower_components/waitForImages": "alexanderdickson/waitForImages#^2.4.0",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "async-es": "^3.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "discord.js": "^11.6.4",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-vue": "^7.1.0",
    "extra-life": "^1.0.5",
    "file-loader": "^6.2.0",
    "gsap": "^3.2.6",
    "html-webpack-plugin": "^4.5.0",
    "mpd": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "odometer": "github:the-ding-stroders/odometer",
    "parallel-webpack": "^2.6.0",
    "reconnecting-websocket": "^4.4.0",
    "request": "^2.88.2",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue": "^2.6.11",
    "vue-animejs": "^2.1.1",
    "vue-loader": "^15.9.5",
    "vue-material": "^1.0.0-beta-15",
    "vue-observe-visibility": "^0.4.6",
    "vue-odometer": "github:the-ding-stroders/vue-odometer",
    "vue-resource": "^1.5.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.26",
    "vuex": "^3.3.0",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "ws": "^7.2.5"
  }
}
